        <item name="140" type="Fixed">
            <dsc>Time of Day</dsc>
            <len>24</len>
            <convert>
                <type>unsigned decimal</type>
                <lsb>1.0/128</lsb>
                <unit>s</unit>
                <max>86400.0</max>
            </convert>
        </item>
        <item name="161">
            <dsc>Track Number</dsc>
            <items>
                <item name="spare1" type="Spare"><len>4</len></item>
                <item name="TRN" type="Fixed">
                    <dsc>Track Number</dsc>
                    <len>12</len>
                </item>
            </items>
        </item>
        <item name="170" type="Extended">
            <dsc>Track Status</dsc>
            <len>(8,8)</len>
            <items>
                <item name="CNF" type="Fixed">
                    <dsc>Confirmed Vs. Tentative Track</dsc>
                    <len>1</len>
                    <values>
                        <value val="0" dsc="Confirmed Track"/>
                        <value val="1" dsc="Tentative Track"/>
                    </values>
                </item>
                <item name="RAD" type="Fixed">
                    <dsc>Type of Sensor(s) Maintaining Track</dsc>
                    <len>2</len>
                    <values>
                        <value val="0" dsc="Combined Track"/>
                        <value val="1" dsc="PSR Track"/>
                        <value val="2" dsc="SSR/Mode S Track"/>
                        <value val="3" dsc="Invalid"/>
                    </values>
                </item>
                <item name="DOU" type="Fixed">
                    <dsc>Signals Level of Confidence in Plot to Track Association Process</dsc>
                    <len>1</len>
                    <values>
                        <value val="0" dsc="Normal confidence"/>
                        <value val="1" dsc="Low confidence in plot to track association"/>
                    </values>
                </item>
                <item name="MAH" type="Fixed">
                    <dsc>Manoeuvre Detection in Horizontal Sense</dsc>
                    <len>1</len>
                    <values>
                        <value val="0" dsc="No horizontal man.sensed"/>
                        <value val="1" dsc="Horizontal man. sensed"/>
                    </values>
                </item>
                <item name="CDM" type="Fixed">
                    <dsc>Climbing / Descending Mode</dsc>
                    <len>2</len>
                    <values>
                        <value val="0" dsc="Maintaining"/>
                        <value val="1" dsc="Climbing"/>
                        <value val="2" dsc="Descending"/>
                        <value val="3" dsc="Unknown"/>
                    </values>
                </item>
                <item name="TRE" type="Fixed">
                    <dsc>Signal for End_of_Track</dsc>
                    <len>1</len>
                    <values>
                        <value val="0" dsc="Track still alive"/>
                        <value val="1" dsc="End of track lifetime(last report for this track)"/>
                    </values>
                </item>
                <item name="GHO" type="Fixed">
                    <dsc>Ghost Vs. True Target</dsc>
                    <len>1</len>
                    <values>
                        <value val="0" dsc="True target track"/>
                        <value val="1" dsc="Ghost target track"/>
                    </values>
                </item>
                <item name="SUP" type="Fixed">
                    <dsc>Track Maintained with Track Information from Neighbouring Node B on the Cluster, or Network</dsc>
                    <len>1</len>
                    <values>
                        <value val="0" dsc="No"/>
                        <value val="1" dsc="Yes"/>
                    </values>
                </item>
                <item name="TCC" type="Fixed">
                    <dsc>Type of Plot Coordinate Transformation Mechanism:</dsc>
                    <len>1</len>
                    <values>
                        <value val="0" dsc="Tracking performed in so-called &apos;Radar Plane&apos;, i.e. neither slant range correction nor stereographical projection was applied"/>
                        <value val="1" dsc="Slant range correction and a suitable projection technique are used to track in a 2D.reference plane, tangential to the earth model at the Radar Site co-ordinates"/>
                    </values>
                </item>
                <item name="spare1" type="Spare"><len>3</len></item>
            </items>
        </item>
