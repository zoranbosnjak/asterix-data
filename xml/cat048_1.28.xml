                </item>
                <item name="SAM" type="Fixed">
                    <dsc>Amplitude of (M)SSR Reply</dsc>
                    <len>8</len>
                    <convert>
                        <type>decimal</type>
                        <lsb>1.0</lsb>
                        <unit>dBm</unit>
                    </convert>
                </item>
                <item name="PRL" type="Fixed">
                    <dsc>Primary Plot Runlength</dsc>
                    <len>8</len>
                    <convert>
                        <type>unsigned decimal</type>
                        <lsb>360.0/8192</lsb>
                        <unit>deg</unit>
                    </convert>
                </item>
                <item name="PAM" type="Fixed">
                    <dsc>Amplitude of Primary Plot</dsc>
                    <len>8</len>
                    <convert>
                        <type>decimal</type>
                        <lsb>1.0</lsb>
                        <unit>dBm</unit>
                    </convert>
                </item>
                <item name="RPD" type="Fixed">
                    <dsc>Difference in Range Between PSR and SSR Plot</dsc>
