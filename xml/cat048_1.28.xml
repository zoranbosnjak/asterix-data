                    <convert>
                        <type>decimal</type>
                        <lsb>1.0/256</lsb>
                        <unit>NM</unit>
                    </convert>
                </item>
                <item name="APD" type="Fixed">
                    <dsc>Difference in Azimuth Between PSR and SSR Plot</dsc>
                    <len>8</len>
                    <convert>
