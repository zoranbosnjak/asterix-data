                    <dsc>Raw Doppler Speed</dsc>
                    <items>
                        <item name="DOP" type="Fixed">
                            <dsc>Doppler Speed</dsc>
                            <len>16</len>
                            <convert>
                                <type>unsigned decimal</type>
                                <lsb>1.0</lsb>
                                <unit>m/s</unit>
                            </convert>
                        </item>
                        <item name="AMB" type="Fixed">
                            <dsc>Ambiguity Range</dsc>
                            <len>16</len>
                            <convert>
                                <type>unsigned decimal</type>
                                <lsb>1.0</lsb>
                                <unit>m/s</unit>
                            </convert>
                        </item>
                        <item name="FRQ" type="Fixed">
                            <dsc>Transmitter Frequency</dsc>
                            <len>16</len>
                            <convert>
                                <type>unsigned decimal</type>
                                <lsb>1.0</lsb>
                                <unit>MHz</unit>
                            </convert>
                        </item>
                    </items>
                </item>
            </items>
        </item>
        <item name="130" type="Compound">
            <dsc>Radar Plot Characteristics</dsc>
            <items>
                <item name="SRL" type="Fixed">
                    <dsc>SSR Plot Runlength</dsc>
